package com.lec.ex2customer;

public class CustomerDTO {
	private int cno;
	private String name;
	private String tel;
	private int point;
	
	public CustomerDTO(int cno, String name, String tel, int point) {
		this.cno = cno;
		this.name = name;
		this.tel = tel;
		this.point = point;
	}
	
	@Override
	public String toString() {
		return cno + "\t" + name + "\t" + tel + "\t" + point + "\n";
	}
	
}